<main class="w-100 h-100 position-absolute customMarginGallery">
  <section class="backgroundColor">
    <div class="container">
      <div class="row">
        <h3 class="text-center text-uppercase mb-5 mt-5 customGallery">Decouvrez ma galerie photo</h3>
        <div class="items row-cols-12 mb-5 d-flex justify-content-center flex-wrap">
          <button class="item active btn btn-secondary me-3 mt-2 d-flex justify-content-center shadow" data-name="Tout">Tout</button>
          <button class="item btn btn-secondary d-flex me-3 mt-2 justify-content-center shadow" data-name="Mariage">Mariage</button>
          <button class="item btn btn-secondary d-flex me-3 mt-2 justify-content-center shadow" data-name="Famille">Famille</button>
