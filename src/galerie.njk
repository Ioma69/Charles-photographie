---
title: CHARLES CANTIN - GALLERIE
layout: 'base2.njk'
---


        <main class="w-100 h-100 position-absolute customMarginGallery">
            <section class="backgroundColor">
              <div class="container">
                <div class="row">
                  <h3 class="text-center text-uppercase mb-5 mt-5 customGallery">Decouvrez ma galerie photo</h3>
                  <div class="items row-cols-12 mb-5 d-flex justify-content-center flex-wrap">
                    <button class="item active btn btn-secondary me-3 mt-2 d-flex justify-content-center shadow" data-name="Tout">Tout</button>
                    <button class="item btn btn-secondary d-flex me-3 mt-2 justify-content-center shadow" data-name="Mariage">Mariage</button>
                    <button class="item btn btn-secondary d-flex me-3 mt-2 justify-content-center shadow" data-name="Famille">Famille</button>
                    <button class="item btn btn-secondary d-flex me-3 mt-2 justify-content-center shadow" data-name="Grossesse">Grossesse</button>
                    <button class="item btn btn-secondary d-flex me-3 mt-2 justify-content-center shadow" data-name="Bapteme">Bapteme</button>
                    <button class="item btn btn-secondary d-flex me-3 mt-2 justify-content-center shadow" data-name="Portrait">Portrait</button>
                    <button class="item btn btn-secondary d-flex me-3 mt-2 justify-content-center shadow" data-name="Couple">Couple</button>
                  </div>
                  <div class="col-6" id="colOne">
                    {% for image in collections.gallery %}
                    <div class="row">
                      <img src="{{image.data.src}}" class="customPhoto image marginAll" data-name="{{image.data.dataName}}" alt="{{image.data.alt}}">
                    </div>
                  
                    <div class="row active">
                      <img src="/images/famille coucher de soleil.jpg" class="customPhoto image show marginAll" data-name="Famille" alt="">
                    </div>
                    <div class="row">
                      <img src="/images/famille allongée.jpg" class="customPhoto image marginAll" data-name="Famille" alt="">
                    </div>
                    <div class="row">
                      <img src="/images/portrait femme.jpg" class="customPhoto image marginAll" data-name="Portrait" alt="">
                    </div>
                    <div class="row">
                      <img src="/images/portrait homme.jpg" class="customPhoto image marginAll" data-name="Portrait" alt="">
                    </div>
                    <div class="row">
                      <img src="/images/couple nuit.jpg" class="customPhoto image marginAll" data-name="Couple" alt="">
                    </div>
                    <div class="row">
                      <img src="/images/couple lecture.jpg" class="customPhoto image marginAll" data-name="Couple" alt="">
                    </div>
                  </div>
                <div class="col-6" id="colTwo">
                  <div class="row">
                    <img src="/images/Couple mariés.jpg" class="customPhoto image marginAll" data-name="Mariage" alt="">
                  </div>
                  <div class="row">
                    <img src="/images/bapteme 3.jpg" class="customPhoto image marginAll" data-name="Bapteme" alt="">
                  </div>
                  <div class="row">
                    <img src="/images/bapteme 2.jpg" class="customPhoto image marginAll" data-name="Bapteme" alt="">
                  </div>
                  <div class="row">
                    <img src="/images/grossesse.jpg" class="customPhoto image marginAll" data-name="Grossesse" alt="">
                  </div>
                  <div class="row">
                    <img src="/images/grossesse 2.jpg" class="customPhoto image marginAll" data-name="Grossesse" alt="">
                  </div>
                  <div class="row">
                    <img src="/images/grossesse 3.jpg" class="customPhoto image marginAll" data-name="Grossesse" alt="">
                  </div>
                </div>
                {% endfor %}
                </div>
              </div>
            </section>
                <footer>
                    <p class="footerStyle text-center text-white mt-5 d-flex flex-column justify-content-center fs-6">@2023 Charles Cantin Photographie <br> Tout Droits Reservés</p>
                </footer>
        </main>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN"
        crossorigin="anonymous"></script>
        <script src="/script.js"></script>

